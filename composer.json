{
    "name": "davidstrada/tagger",
    "description": "Eloquent tagging package",
    "license": "MIT",
    "authors": [
        {
            "name": "David Strada",
            "email": "idavid@fastmail.com",
            "homepage": "https://davidstrada.com",
            "role": "Developer"
        }
    ],
    "homepage": "https://github.com/davidstrada/taggy",
    "keywords": ["Laravel", "Tagger"],
    "require": {
        "php": "^7.4",
        "illuminate/support": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "orchestra/testbench": "^5.0",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "DavidStrada\\Tagger\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DavidStrada\\Tagger\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "DavidStrada\\Tagger\\Providers\\TaggerServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
